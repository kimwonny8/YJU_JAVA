package Main;

import java.util.Scanner;
//2101059 ±èÁ¤¿ø #2
public class StarTest {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		
		for(int i=0; i<n; i++) {
			for(int j=i; j>0; j--) {
				if(j<i) System.out.print(" ");
				else System.out.print("*");
			}
			System.out.println();
		}
	}
}